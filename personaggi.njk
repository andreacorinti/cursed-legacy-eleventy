<h1>Personaggi</h1>

{% for post in collections.personaggi %}
<div class="responsive">
  <div class="gallery">
    <a href="{{ post.url | url }}">
      <img src="/{{ post.data.immagine }}" alt="{{ post.data.title }}" width="600" height="400">
    </a>
    <div class="desc">{{ post.data.title }}</div>
  </div>
</div>
{% endfor %}

<div class="clearfix"></div>
<div style="padding:6px;">